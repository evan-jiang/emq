<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xmlns:mvc="http://www.springframework.org/schema/mvc"
    xsi:schemaLocation="http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-4.0.xsd
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

	


    <bean id="config" class="com.tdpark.config.Config">
    	<property name="nodeIdx"  value="1"/><!-- 当前节点编号  -->
    	<property name="nodeTotal" value="1"/><!-- 总节点数 -->
    	<property name="threadNum"  value="64"/><!-- 每个节点的线程数 -->
    	<!-- <property name="nodes">所有节点的访问根路径
    		<map>
    			<entry key="1" value="http://127.0.0.1:1080/"/>
    		</map>
    	</property> -->
    	<property name="domaines"><!-- 允许消费消息的域名 -->
    		<map>
    			<entry key="http://127.0.0.1:1080/" value="true"/>
    		</map>
    	</property>
    </bean>
</beans>
